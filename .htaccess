Options All -Indexes

RewriteEngine On

RewriteRule ^([-a-zA-Z0-9/]+)$ index.php?ruta=$1

# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:app.ifixitmor.com
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^app.ifixitmor.com$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
